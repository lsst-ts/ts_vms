gcc  -fPIC -g -g -m64 -D_REENTRANT -Wall -I"." -I"/home/dmills/ts_opensplice/OpenSpliceDDS/V6.9/HDE/x86_64.linux/examples/include" -I"/home/dmills/ts_opensplice/OpenSpliceDDS/V6.9/HDE/x86_64.linux/examples" -I"/home/dmills/ts_opensplice/OpenSpliceDDS/V6.9/HDE/x86_64.linux/include" -I"/home/dmills/ts_opensplice/OpenSpliceDDS/V6.9/HDE/x86_64.linux/include/sys" -I"/home/dmills/ts_opensplice/OpenSpliceDDS/V6.9/HDE/x86_64.linux/include/dcps/C++/SACPP" -I"/home/dmills/ts_sal/lsstsal/include" -I"${SAL_WORK_DIR}/MTVMS/cpp/src"  -I"${SAL_WORK_DIR}/MTVMS/cpp" -I"${SAL_WORK_DIR}/include" -Wno-write-strings -I../Include -I../Publisher -I../FPGA -I../Logging -I ../Settings -I../Utility -c  -o "NiFpga.o" NiFpga.c

